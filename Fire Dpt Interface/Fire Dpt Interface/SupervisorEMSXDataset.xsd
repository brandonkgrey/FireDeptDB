                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        [Employee Information].Name, [EMS Certifications].Employee_ID, [EMS Certifications].[TDH Level], [EMS Certifications].[TDH Level Expiration Date], ROUND([EMS Certifications].[TDH Level Expiration Date] - now, 
                         0) AS Expr1
FROM            ([Employee Information] INNER JOIN
                         [EMS Certifications] ON [Employee Information].Employee_ID = [EMS Certifications].Employee_ID)
WHERE        ([EMS Certifications].[TDH Level] &lt;&gt; 'N/A') AND (ROUND([EMS Certifications].[TDH Level Expiration Date] - now, 0) &lt;= 90) AND ([Employee Information].[End Date] IS NULL) AND 
                         ([Employee Information].Ops = False) AND ([Employee Information].[Authorization] = '1' OR
                         [Employee Information].[Authorization] = '2')
ORDER BY [EMS Certifications].[TDH Level Expiration Date]</CommandText>
