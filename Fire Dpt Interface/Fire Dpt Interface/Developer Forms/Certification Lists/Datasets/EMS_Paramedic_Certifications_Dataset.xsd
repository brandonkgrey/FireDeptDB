                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        [Employee Information].Name, [Employee Information].Employee_ID, [Employee Information].[End Date], [Employee Information].[TDH ID], [EMS Certifications].[TDH Level], 
                         [EMS Certifications].[TDH Level Expiration Date], [Employee Information].Ops
FROM            ([Employee Information] INNER JOIN
                         [EMS Certifications] ON [Employee Information].Employee_ID = [EMS Certifications].Employee_ID)
WHERE        ([Employee Information].[End Date] IS NULL) AND ([EMS Certifications].[TDH Level] = 'EMT-Paramedic') AND ([Employee Information].Ops = False) OR
                         ([Employee Information].[End Date] IS NULL) AND ([EMS Certifications].[TDH Level] = 'Licensed Paramedic')
