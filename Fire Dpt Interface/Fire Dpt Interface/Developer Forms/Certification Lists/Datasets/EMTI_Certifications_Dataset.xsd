                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        [Employee Information].Name, [Employee Information].Employee_ID, [Employee Information].SSN, [Employee Information].[TDH ID], [EMS Certifications].[TDH Level], 
                         [EMS Certifications].[TDH Level Expiration Date], [EMS Certifications].[TDH Instructor], [EMS Certifications].[TDH Instructor Expiration Date], [EMS Certifications].[TDH Coordinator], 
