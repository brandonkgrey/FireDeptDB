                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        [Employee Information].Employee_ID, [Employee Information].Name, [Employee Information].[End Date], [Assignment Pay].ARFF, [Class Information].[Start Date], [Class Information].[FAA Subject Area], 
                         [EMS Certifications].[TDH Level], [EMS Certifications].[TDH Level Expiration Date], [Class Information].[Class Name], [Class Information].[Class Number], [Class Information].[CE Hours]
FROM            (([ARFF Subject Area] INNER JOIN
                         (([Employee Information] INNER JOIN
                         ([Class Information] INNER JOIN
                         [Class Attendance Information] ON [Class Information].[Class Number] = [Class Attendance Information].[Class Number]) ON [Employee Information].Employee_ID = [Class Attendance Information].Employee_ID) 
                         INNER JOIN
                         [Assignment Pay] ON [Employee Information].Employee_ID = [Assignment Pay].Employee_ID) ON [ARFF Subject Area].ARFF = [Class Information].[FAA Subject Area]) INNER JOIN
                         [EMS Certifications] ON [Employee Information].Employee_ID = [EMS Certifications].Employee_ID)
WHERE        ([Employee Information].Employee_ID = ?) AND ([Employee Information].[End Date] IS NULL) AND ([Assignment Pay].ARFF = True) AND ([Class Information].[Start Date] &gt;= ?) AND 
                         ([Class Information].[Start Date] &lt;= ?)</CommandText>
