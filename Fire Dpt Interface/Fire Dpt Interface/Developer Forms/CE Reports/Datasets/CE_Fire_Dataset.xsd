                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        [Class Attendance Information].Employee_ID, [Employee Information].Name, [Employee Information].[End Date], [Employee Information].[TCFP PIN], [Class Information].[Fire Credit], [Class Information].[Start Date], 
                         [Class Information].[Class Name], [Class Information].[Class Number], [Class Information].[Structural FF Hours], [Class Information].[Driver Hours], [Class Information].[Fire Officer Hours], 
                         [Class Information].[HazMat Hours], [Class Information].[ARFF Hours], [Class Information].[Rescue Hours], [Class Information].[Inspector Hours], [Class Information].[Investigator Hours], 
                         [Class Information].[Wildland Hours], [Class Information].[Instructor Hours], [Class Information].[Safety Hours], [Class Information].[Head of Department Hours], [Class Information].Track
FROM            ([Employee Information] INNER JOIN
                         ([Class Information] INNER JOIN
                         [Class Attendance Information] ON [Class Information].[Class Number] = [Class Attendance Information].[Class Number]) ON [Employee Information].Employee_ID = [Class Attendance Information].Employee_ID)
WHERE        ([Class Attendance Information].Employee_ID = ?) AND ([Class Information].[Fire Credit] = True) AND ([Class Information].[Start Date] &gt;= ?) AND ([Class Information].[Start Date] &lt;= ?)
ORDER BY [Class Information].[Start Date]</CommandText>
